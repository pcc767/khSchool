package com.kh.example.practice2.model.vo;

public class Product {
	
	private String pName;
	private int price;
	private String brand;
	
	public Product() {
		
	}
	
	public void information(String pName,int price,String brand) {
		this.pName = pName;	
		this.price = price;	
		this.brand = brand;	
	}
	


}
